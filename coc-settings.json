{
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "html",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "json",
        "jsonc",
        "python",
        "graphql"
    ],
    "coc.preferences.formatOnType": true,
    "prettier.arrowParens": "avoid",
    "prettier.printWidth": 100,
    "prettier.tabWidth": 2,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "all"
}
