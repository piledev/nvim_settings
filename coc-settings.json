{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "go",
    "python",
    "graphql"
  ],
  "coc.preferences.formatOnType": true,
  "prettier.arrowParens": "avoid",
  "prettier.printWidth": 80,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "eslint.enable": false
}
